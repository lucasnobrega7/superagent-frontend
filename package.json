{
  "name": "superagent-frontend",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@11.3.0",
  "scripts": {
    "dev": "next dev",
    "turbo:dev": "npx turbo dev",
    "build": "next build",
    "turbo:build": "npx turbo build",
    "start": "next start",
    "lint": "next lint",
    "turbo:lint": "npx turbo lint",
    "test": "jest",
    "turbo:test": "npx turbo test",
    "test:watch": "jest --watch",
    "test:api": "node ./app/api/v1/agents/__tests__/run-api-tests.js",
    "functions:build": "npm --prefix \"functions\" run build",
    "functions:serve": "npm --prefix \"functions\" run serve",
    "functions:deploy": "npm --prefix \"functions\" run deploy",
    "functions:logs": "npm --prefix \"functions\" run logs",
    "functions:shell": "npm --prefix \"functions\" run shell",
    "clean": "rm -rf node_modules .next .turbo",
    "build:functions": "npm --prefix \"functions\" run build"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.19.0",
    "@literalai/client": "^0.1.5",
    "@sentry/browser": "^9.17.0",
    "@sentry/nextjs": "^9.17.0",
    "@sentry/node": "^9.17.0",
    "@sentry/tracing": "^7.120.3",
    "@supabase/supabase-js": "^2.22.0",
    "axios": "^1.4.0",
    "firebase": "10.9.0",
    "firebase-admin": "^13.3.0",
    "next": "^15.3.2",
    "openai": "^4.98.0",
    "posthog-js": "^1.49.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.2.4",
    "@types/supertest": "^6.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.42.0",
    "eslint-config-next": "^13.4.4",
    "ignore-loader": "^0.1.2",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "postcss": "^8.4.24",
    "supertest": "^7.1.0",
    "tailwindcss": "^3.3.2",
    "ts-jest": "^29.3.2",
    "turbo": "^2.5.3",
    "typescript": "^5.1.3"
  }
